{
  "name": "avatarbox.sdk",
  "version": "1.0.0",
  "description": "nodejs sdk for avatarbox.io",
  "main": "Release/Presentation/index.js",
  "types": "Release/Presentation/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jasmine-ts --config=jasmine.json",
    "test:cov": "nyc -r text -e .ts -x \"**/*.spec.ts\" npm run test",
    "postinstall": "npm run build",
    "format": "prettier --write \"**/*.ts\"",
    "coverage": "NODE_ENV=test nyc -r text-lcov -e .ts -x \"**/*.spec.ts\" npm run test | coveralls",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrtillman/avatarbox.io.git"
  },
  "keywords": [
    "avatar",
    "box",
    "node",
    "sdk"
  ],
  "author": "mrtillman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrtillman/avatarbox.io/issues"
  },
  "homepage": "https://github.com/mrtillman/avatarbox.io#readme",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.3.0",
    "@aws-sdk/client-kms": "^3.3.0",
    "@aws-sdk/client-s3": "^3.5.0",
    "@aws-sdk/client-sqs": "^3.4.1",
    "@types/jasmine": "^3.5.0",
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.7",
    "dotenv": "^8.2.0",
    "expect": "^24.9.0",
    "grav.client": "^2.4.19",
    "jasmine": "^3.5.0",
    "moment": "^2.29.1",
    "nock": "^11.7.2",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.1.0",
    "coveralls": "^3.0.9",
    "jasmine-ts": "^0.3.0",
    "mem": ">=4.0.0",
    "minimatch": ">=3.0.2",
    "minimist": ">=0.2.1",
    "nyc": "^15.0.0",
    "prettier": "^2.0.4",
    "snyk": "^1.319.0",
    "ts-node": "^8.6.2",
    "yargs-parser": ">=18.1.2"
  },
  "snyk": true
}
